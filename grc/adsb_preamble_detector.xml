<?xml version="1.0"?>
<block>
  <name>preamble_detector</name>
  <key>adsb_preamble_detector</key>
  <category>[adsb]</category>
  <import>import adsb</import>
  <make>adsb.preamble_detector($samp_rate,$thres)</make>

  <param>
    <name>sampling rate</name>
    <key>samp_rate</key>
    <type>real</type>
  </param>
  <param>
    <name>Threshold</name>
    <key>thres</key>
    <type>float</type>
  </param>
  
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
   <sink>
    <name>mov_average</name>
    <type>float</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
  <source>
    <name>trigger</name>
    <type>short</type>
  </source>
  
</block>
