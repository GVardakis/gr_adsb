<?xml version="1.0"?>
<block>
  <name>preamble_detector</name>
  <key>adsb_preamble_detector</key>
  <category>[adsb]</category>
  <import>import adsb</import>
  <make>adsb.preamble_detector($samp_rate,$pulse_dur,$center_freq)</make>

  <param>
    <name>sampling rate</name>
    <key>samp_rate</key>
    <type>real</type>
  </param>
  <param>
    <name>pulse duration</name>
    <key>pulse_dur</key>
    <type>int</type>
  </param>
  <param>
    <name>center frequency</name>
    <key>center_freq</key>
    <type>real</type>
  </param>
  
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
